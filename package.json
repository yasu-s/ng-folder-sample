{
  "name": "ng-sample",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "yarn && ng serve",
    "build": "yarn && ng build",
    "test": "yarn && ng test --browsers ChromeDebug",
    "test:headless": "yarn && ng test --code-coverage --browsers ChromeHeadless --watch false",
    "test:no-browser": "yarn && ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn test:headless"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.3",
    "@angular/common": "~8.2.3",
    "@angular/compiler": "~8.2.3",
    "@angular/core": "~8.2.3",
    "@angular/forms": "~8.2.3",
    "@angular/platform-browser": "~8.2.3",
    "@angular/platform-browser-dynamic": "~8.2.3",
    "@angular/router": "~8.2.3",
    "core-js": "^2.6.5",
    "rxjs": "~6.5.2",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.0",
    "@angular/cli": "^8.3.0",
    "@angular/compiler-cli": "~8.2.3",
    "@angular/language-service": "^8.2.3",
    "@types/jasmine": "~3.3.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.12.10",
    "codelyzer": "~5.1.0",
    "husky": "^1.3.1",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.5",
    "karma-jasmine": "~2.0.0",
    "karma-jasmine-html-reporter": "^1.4.0",
    "prettier": "^1.16.1",
    "prettier-stylelint": "^0.4.2",
    "protractor": "~5.4.0",
    "stylelint-config-idiomatic-order": "^6.2.0",
    "ts-node": "~8.0.0",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "~3.5.3"
  }
}
